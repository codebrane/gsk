<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="attrEncrypter" class="org.guanxi.idp.farm.rule.AttributeEncrypter">
    <property name="ruleName"><value>encrypt</value></property>
  </bean>

  <bean id="domainAppender" class="org.guanxi.idp.farm.rule.DomainAppender">
    <property name="ruleName"><value>append_domain</value></property>
  </bean>

</beans>
